    "version": "0.2.0",
    "configurations": [
        {
            "name": "Create SRE Issue",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/scripts/create_sre_issue",
            "args": [
                "-c ${workspaceFolder}"
            ],
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Apply Deployments",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/scripts/apply_deploy.py",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Validate Issue",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/common/validations/issues",
        },
        {
            "name": "Sonar Scanner",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/ci/sonar_scanner.py",
            "args": [
                "-t SDL",
            ]
        },       {
            "name": "Validate Changelog",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/common/validations/changelog",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Validate Buildable Project",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/common/validations/buildable"
        },
        {
            "name": "Deploy Helm",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/helm/deploy.py",
            "envFile": "${workspaceFolder}/.env",
        },
		{
			"name": "Validate Translation",
			"type": "python",
			"request": "launch",
			"console": "integratedTerminal",
            "cwd": "/workspaces",
			"program": "${workspaceFolder}/translations/validation",
            "envFile": "${workspaceFolder}/.env",
		},
        {
            "name": "Release Translations",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/translations/release",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Sync Deployments",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/argocd/sync_deployments",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Update Deployments Image",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/argocd/update_deployments_image",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Deploy Maven",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/maven/deploy",
            "envFile": "${workspaceFolder}/.env",
        },
        {
            "name": "Flutter Publish",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces/dart-mobile-clocking-event",
            "program": "${workspaceFolder}/mobile/flutter/libs",
            "args": [
                "-p",
            ]
        },
        {
            "name": ".NET Generate",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/dotnet/apps",
            "args": [
                "-g service-name",
            ]
        },
        {
            "name": ".NET Snapshot",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/dotnet/apps",
            "args": [
                "-r",
                "snapshot"
            ]
        },
        {
            "name": "Set Version",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/set_version.py",
            "envFile": "${workspaceFolder}/.env",
            "args": [
                "3.2.5"
            ],
            "env": {
                "PYTHONPATH": "senior-ci"
            }
        },
        {
            "name": "Generate",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/generate.py",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "senior-ci"
            }
        },
        {
            "name": "Unit Test",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/unit_test.py",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "senior-ci"
            }
        },
        {
            "name": "Compile",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/compile.py",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "senior-ci"
            }
        },
        {
            "name": "SAST",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/sonar_scanner.py",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "senior-ci"
            }
        },
        {
            "name": "Package",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/src/main/package.py",
            "envFile": "${workspaceFolder}/.env",
            "env": {
                "PYTHONPATH": "senior-ci"
            },
            "sudo": true
        },
        {
            "name": "Test A",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "program": "${workspaceFolder}/aaa.py",
        },
        {
            "name": "Test B",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/bbb.py",
        },
        {
            "name": "Test C",
            "type": "python",
            "request": "launch",
            "console": "integratedTerminal",
            "cwd": "/workspaces",
            "program": "${workspaceFolder}/ccc.py",
        }
